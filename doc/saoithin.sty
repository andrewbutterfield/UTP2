\def\COZ#1{\\&&\coz{#1}}
\def\EQ#1{\\&=&\coz{#1}}
\def\NEQ#1{\\&\neq&\coz{#1}}
\def\IMP#1{\\&\implies&\coz{#1}}
\def\PMI#1{\\&\impliedby&\coz{#1}}
\def\REF#1{\\&\refines&\coz{#1}}
\def\EQV#1{\\&\equiv&\coz{#1}}
\def\EQm#1{\\&=&\mcoz{#1}}
\def\IMPm#1{\\&\implies&\mcoz{#1}}
\def\PMIm#1{\\&\impliedby&\mcoz{#1}}
\def\REFm#1{\\&\refines&\mcoz{#1}}
\def\EQVm#1{\\&\equiv&\mcoz{#1}}
